 if( headA==null){
            return headB;
        }
        if( headB==null){
            return headA;
        }
        Set<ListNode> set= new HashSet<>();
        ListNode curr1= headA;
         ListNode curr2= headB;
        while(curr1!=null){
            set.add(curr1);
            curr1=curr1.next;
        }
        boolean flag= true;
        while(curr2!=null){
            flag=set.add(curr2);
            if(flag==false){
                return curr2;
            }
            curr2=curr2.next;
        }



