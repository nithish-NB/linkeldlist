  if( headA==null){
            return headB;
        }
        if( headB==null){
            return headA;
        }

        ListNode curr1= headA;
        while(curr1!=null){
            ListNode curr2= headB;
            while( curr2!=null){
                if( curr1==curr2){
                    return curr1;
                }
                curr2=curr2.next;
            }
            curr1=curr1.next;
        }
