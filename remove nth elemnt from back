class Solution {
    public ListNode removeNthFromEnd(ListNode head, int n) {
        if( head==null|| head.next==null){
            return null;
        }
        ListNode dummy= new ListNode(0);
        ListNode fast= dummy;
        ListNode slow = dummy;
        slow.next=head;
        for(int i=1;i<=n+1;i++){
            fast=fast.next;
        }
        while(fast!=null){
            fast=fast.next;
            slow=slow.next;
        }
        slow.next=slow.next.next;
    
        
   //return head;}
        return dummy.next;}
}
